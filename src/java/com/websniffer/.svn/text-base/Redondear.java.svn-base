package com.websniffer;

/**
 * Clase para redondear
 * @author skuarch
 */
public class Redondear {


    //=================================
    /**
     * metodo que redondea un double
     * @param num Double
     * @param ndecimal int
     * @return double
     */
    public double redondear(double num, int ndecimal) {
        double aux0 = Math.pow(10, ndecimal);
        double aux = num * aux0;
        int tmp = (int) aux;

        return (double) (tmp / aux0);
    }//fin redondear

}//fin class
