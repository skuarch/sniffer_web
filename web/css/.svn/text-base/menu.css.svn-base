a.button{
    background:url(../images/button.gif);
    display:block;
    color:#555555;
    font-weight:bold;
    height:30px;
    line-height:29px;
    margin-bottom:7px;
    text-decoration:none;
    width:132px;
    font-size: 10.5px;
}
a:hover.button{
    /*color:#0066CC;*/
    color: white;
}

