body{
    font-family:"Lucida Grande", "Lucida Sans Unicode", Verdana, Arial, Helvetica, sans-serif;
    font-size:11px;
    margin: 0;
}

#estado{

    width: 950px;
    height: 20px;
    float: right; 
    margin-top: 5px;
    font-size: 7pt;

}


/* Logo */

#logo {
	float: left;
}

#logo h1 {
	margin: 0;
	padding: 8px 0 0 20px;
	font-size: 2em;
	font-weight: normal;
	color: #514f42;
        font-family: sans-serif;
}

#logo h1 a {
	display: block;
	text-decoration: none;
	color: #514f42;
}

#logo a {
	display: block;
	text-decoration: none;
	color: #FFF;
}


.bordes{
	border:1px solid #cccccc;

}


/* estilo para los vinculos**************************************************/


a:link {
	color:#000000;
	text-decoration: none;
	font-weight:bold;

}
a:visited {
	text-decoration: none;
	color:#01B1D0;
	font-weight:bold;

}
a:hover {
	text-decoration: none;
	color:#ffffff;
	font-weight:bold;


}
a:active {
	text-decoration: none;
	color:#01B1D0;
	font-weight:bold;

}


.div_cargador{

    padding: 190px 0 0 0;
    width: 790px;
    height: 390px;
    
}

.categories{
    text-decoration: none;
    font-weight: normal;
    font-size: 11pt;
    background-color: #eaeaea;
}

.categories a:link {color: #000000; text-decoration: none; font-weight: normal;}
.categories a:visited {color: #000000; text-decoration: none; font-weight: normal;}
.categories a:hover {color: #ffffff; text-decoration: none ;font-weight: normal;}
.categories a:active {color: #000000; font-weight: normal;}

.barVolumen{
    background-color: #FDFEDC;
    width: 789px;
    margin-bottom: 8px;
}


.ejemplo{
    font-size: 11px;
    color: #cccccc;
}