


function changeE2E(selectValue){    

    if(selectValue == "personalizar"){

        //se tiene que desahablitar el destino
        document.formE2E.destinoPersonalizado.disabled = false;

        if(document.formE2E.destinoPersonalizado.value == ""){

            document.formE2E.destinoPersonalizado.value="host destino";

        }

    }else{

        document.formE2E.destinoPersonalizado.disabled = true;
        document.formE2E.destinoPersonalizado.value="";

    }


}//fin function