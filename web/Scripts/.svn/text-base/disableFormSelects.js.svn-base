/**
 * this function disable only formSelects
 */
function disableFormSelects(){

    if(tipoGrafica == "E2E"){

        document.formSelects.selectCollector.disabled = true;
        document.formSelects.selectMeasure.disabled = true;
        document.formSelects.selectDates.disabled = true;
        document.formSelects.selectOptions.disabled = true;
        document.formSelects.aplicar.disabled = true;

    } else if(tipoGrafica == "ALARMAS"){

        document.formSelects.selectOptions.disabled = true;

    } else if(tipoGrafica == "SERVIDORES"){

        document.formSelects.selectOptions.disabled = true;

    } else if(tipoGrafica == "INDICADORES"){

        document.formSelects.selectOptions.disabled = true;

    }else{

        document.formSelects.selectCollector.disabled = false;
        document.formSelects.selectMeasure.disabled = false;
        document.formSelects.selectDates.disabled = false;
        document.formSelects.selectOptions.disabled = false;
        document.formSelects.aplicar.disabled = false;

    }

}
